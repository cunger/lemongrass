SELECT DISTINCT ?thirdPerson_plural_present WHERE { 

  { <URI> <http://lemon-model.net/lemon#canonicalForm> ?form . } 
  UNION 
  { <URI> <http://lemon-model.net/lemon#otherForm> ?form . } 
 
  ?form <http://www.lexinfo.net/ontology/2.0/lexinfo#person> <http://www.lexinfo.net/ontology/2.0/lexinfo#thirdPerson> . 
  ?form <http://www.lexinfo.net/ontology/2.0/lexinfo#number> <http://www.lexinfo.net/ontology/2.0/lexinfo#plural> . 
  ?form <http://www.lexinfo.net/ontology/2.0/lexinfo#tense> <http://www.lexinfo.net/ontology/2.0/lexinfo#present> . 
  ?form <http://lemon-model.net/lemon#writtenRep> ?thirdPerson_plural_present . 
}
